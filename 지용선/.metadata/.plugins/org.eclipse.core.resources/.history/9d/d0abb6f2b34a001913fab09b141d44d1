package common;

import billing.CashManager;

public class User {
	private CashManager     cash;
	private HoldItemManager holdItem;
	
	public CashManager getCash() {
		return cash;
	}
	public void setCash(CashManager cash) {
		this.cash = cash;
	}
	public HoldItemManager getHoldItem() {
		return holdItem;
	}
	public void setHoldItem(HoldItemManager holdItem) {
		this.holdItem = holdItem;
	}
	
	
}
